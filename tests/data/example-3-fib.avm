; let rec fib n =
;   if n <= 1
;   then 1
;   else fib (n - 2) + fib (n - 1)
; in fib 30
main:
    dum 1
    clos fib
    upd 0			; env = [fib]
    mark
    load 30 push
    acc 0
    app
    endlet
    ret
fib:
    load 1 push			; env = [n fib]
    acc 0
    le
    bf L00
    load 1
    ret
L00:
    mark
    load 1 push
    acc 0
    sub push
    acc 1
    app push
    
    mark
    load 2 push
    acc 0
    sub push
    acc 1
    app

    add
    ret