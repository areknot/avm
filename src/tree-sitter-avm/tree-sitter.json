{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "avm",
      "camelcase": "AVM",
      "title": "AVM",
      "scope": "source.avm",
      "file-types": [
        "avm"
      ],
      "injection-regex": "^avm$",
      "class-name": "TreeSitterAvm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "Apache-2.0",
    "description": "The parser for AVM",
    "authors": [
      {
        "name": "Zhangfan Li",
        "email": "dzangfan.li@gmail.com",
        "url": "dzangfan.github.io"
      }
    ],
    "links": {
      "repository": "https://github.com/areknot/avm",
      "funding": ""
    }
  },
  "bindings": {
    "c": true,
    "go": false,
    "java": false,
    "node": false,
    "python": false,
    "rust": false,
    "swift": false,
    "zig": false
  }
}